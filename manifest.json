{
  "manifest_version": 2,

  "name": "tweekPage",
  "description": "This extension allow you to tweek pages",
  "version": "1.0",

  "content_scripts": [ {
    "js": [ "jquery-1.11.3.js", "tweek.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}